<?xml version="1.0" encoding="UTF-8"?>
<Common>
    <LoginLogout>
        <Login>
            <textUsername>
                <identifier>xpath</identifier>
                <Element>//*[@id="user-name"]</Element>
            </textUsername>
            <textPassword>
                <identifier>xpath</identifier>
                <Element>//*[@id="password"]</Element>
            </textPassword>
            <buttonLogin>
                <identifier>xpath</identifier>
                <Element>//*[@id="login-button"]</Element>
            </buttonLogin>
            <header>
                <identifier>xpath</identifier>
                <Element>/html/body/div[1]/div/div/div[1]/div[1]/div[2]/div</Element>
            </header>
            <lockedOutUserErrorMessage>
                <identifier>class</identifier>
                <Element>error-button</Element>
            </lockedOutUserErrorMessage>
        </Login>
        <Logout>
            <slider>
            <menu>
                <identifier>xpath</identifier>
                <Element>//*[@id="react-burger-menu-btn"]</Element>
            </menu>
                <btnAboutUs>
                    <identifier>xpath</identifier>
                    <Element>//*[@id="about_sidebar_link"]</Element>
                </btnAboutUs>
            </slider>
            <Filter>
                <filterDropDown>
                    <identifier>xpath</identifier>
                    <Element>/html/body/div[1]/div/div/div[1]/div[2]/div[2]/span/select</Element>
                </filterDropDown>
                <filterbyNameAtoZ>
                    <identifier>xpath</identifier>
                    <Element>/html/body/div[1]/div/div/div[1]/div[2]/div[2]/span/select</Element>
                </filterbyNameAtoZ>
            </Filter>
            <AboutUsPage>
                <btnTryForFree>
                    <identifier>xpath</identifier>
                    <Element>/html/body/div[2]/div[1]/section[1]/div/div/div/div[1]/div/div[2]/ul/li[1]/a</Element>
                </btnTryForFree>
            </AboutUsPage>
            <btnLogount>
                <identifier>xpath</identifier>
                <Element>///*[@id="logout_sidebar_link"]</Element>
            </btnLogount>
        </Logout>
    </LoginLogout>
    <Products>
        <product1>
            <identifier>xpath</identifier>
            <Element>/html/body/div[1]/div/div/div[2]/div/div/div/div[1]/div[1]/a/img</Element>
        </product1>
        <product2>
            <identifier>xpath</identifier>
            <Element>/html/body/div[1]/div/div/div[2]/div/div/div/div[2]/div[1]/a/img</Element>
        </product2>
        <btnAddToCart>
            <identifier>xpath</identifier>
            <Element>//*[@id="add-to-cart-sauce-labs-backpack"]</Element>
        </btnAddToCart>

        <btnRemoveFromCart>
            <identifier>xpath</identifier>
            <Element>//*[@id="remove-sauce-labs-backpack"]</Element>
        </btnRemoveFromCart>
        <btnTrolley>
            <identifier>xpath</identifier>
            <Element>/html/body/div[1]/div/div/div[1]/div[1]/div[3]/a</Element>
        </btnTrolley>
        <btnCheckOut>
            <identifier>xpath</identifier>
            <Element>//*[@id="checkout"]</Element>
        </btnCheckOut>

    </Products>
    <CustomerDetails>
        <txtFirstName>
            <identifier>xpath</identifier>
            <Element>//*[@id="first-name"]</Element>
        </txtFirstName>
        <txtLastName>
            <identifier>xpath</identifier>
            <Element>//*[@id="last-name"]</Element>
        </txtLastName>
        <txtZipCode>
            <identifier>xpath</identifier>
            <Element>//*[@id="postal-code"]</Element>
        </txtZipCode>
        <btnContinue>
            <identifier>xpath</identifier>
            <Element>//*[@id="continue"]</Element>
        </btnContinue>
        <btnFinish>
            <identifier>xpath</identifier>
            <Element>//*[@id="finish"]</Element>
        </btnFinish>
    </CustomerDetails>
    <btnBackHome>
        <identifier>xpath</identifier>
        <Element>//*[@id="back-to-products"]</Element>
    </btnBackHome>
    <OrderCompleted>

    </OrderCompleted>

</Common>
